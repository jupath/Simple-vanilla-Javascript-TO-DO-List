{
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "airbnb",
    "rules": {
        "no-console": 0,
        "no-unused-vars": 1,
        "func-names": 0,
        "comma-dangle": 0,
        "max-len": 0,
        "no-param-reassign": [
            2,
            {
              "props": false
            }
          ]
    }
}
